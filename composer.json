{
	"name": "brightflair/spektrix-api",
	"description": "Self-documenting, type-safe Spektrix web API.",
	"require": {
		"ext-curl": "*",
		"phpgt/fetch": "^1.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5",
		"phpstan/phpstan": "^1.10",
		"phpmd/phpmd": "^2.13",
		"squizlabs/php_codesniffer": "^3.7"
	},
	"autoload": {
		"psr-4": {
			"BrightFlair\\SpektrixAPI\\": "./src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"BrightFlair\\SpektrixAPI\\Test\\": "./test/phpunit"
		}
	},
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/g105b"
		}
	]
}
